{
    "problemMatcher": [
        {
            "owner": "svelte-check",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):(\\d+) - (error|warning) (.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        }
    ]
}
